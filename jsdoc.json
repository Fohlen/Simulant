{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [ "src/" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
	  "readme": "README.md",
      "encoding": "utf8",
      "destination": "./doc/",
      "recurse": true
    }
}

