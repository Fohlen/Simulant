{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [ "src/", "README.md" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./doc/",
      "private": true,
      "recurse": true,
      "template": "./node_modules/minami"      
    }
}

